        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="217dp"
            android:layout_height="186dp"
            android:src="@drawable/kopi_dampit" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="222dp"
